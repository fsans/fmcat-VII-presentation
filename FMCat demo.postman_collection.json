				{
					"name": "Get session token",
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
					"request": {
						"auth": {
							"type": "basic",
							"basic": [
								{
									"key": "username",
									"value": "{{user}}",
									"type": "string"
								},
								{
									"key": "password",
									"value": "{{password}}",
									"type": "string"
								}
							]
						},
						"method": "GET",
						"header": [
							{
								"key": "Content-Type",
								"name": "Content-Type",
								"value": "application/x-www-form-urlencoded",
								"type": "text"
							}
						],
						"body": {},
						"url": {
							"raw": "https://{{server}}/fmi/data/v1/databases/{{database}}/sessions",
							"protocol": "https",
							"host": [
								"{{server}}"
							],
							"path": [
								"fmi",
								"data",
								"v1",
								"databases",
								"{{database}}",
								"sessions"
							]
						}
					},
					"response": []
				},
				{
					"name": "Get records",
					"protocolProfileBehavior": {
						"disableBodyPruning": true
					},
					"request": {
						"method": "GET",
						"header": [
							{
								"key": "Authorization",
								"value": "Bearer {{token}}",
								"type": "text"
							}
						],
						"body": {},
						"url": {
							"raw": "https://{{server}}/fmi/data/v1/databases/{{database}}/sessions/layouts/{{layout}}/records",
							"protocol": "https",
							"host": [
								"{{server}}"
							],
							"path": [
								"fmi",
								"data",
								"v1",
								"databases",
								"{{database}}",
								"sessions",
								"layouts",
								"{{layout}}",
								"records"
							]
						}
					},
					"response": []
				}
			]
		},
